	"shader_model": {
		"code": "",
		"global": "// The MIT License, Copyright Â© 2021 Inigo Quilez, https://www.shadertoy.com/view/flSSDy\nfloat sdTunnel(vec2 p, vec2 wh) {\n    p.x = abs(p.x);\n    vec2 q = p - wh;\n\tvec2 v1 = vec2(max(q.x,0.0),q.y);\n    float d1 = dot(v1, v1);\n    q.x = (p.y>0.0) ? q.x : length(p)-wh.x;\n\tvec2 v2 = vec2(q.x,max(q.y,0.0));\n    float d2 = dot(v2, v2);\n    float d = sqrt( min(d1,d2) );\n    \n    return (max(q.x,q.y)<0.0) ? -d : d;\n}\n",
		"inputs": [

		],
		"instance": "",
		"longdesc": "A tunnel shape described as a signed distance function",
		"name": "sdTunnel",
		"outputs": [
			{
				"longdesc": "The generated signed distance function",
				"sdf2d": "sdTunnel($uv-vec2($cx+0.5, $cy+0.5), vec2($w, $h))",
